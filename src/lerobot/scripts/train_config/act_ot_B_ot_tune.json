{
  "dataset": {
    "repo_id": "left_fr3_ip_dst_q2cq",
    "root": "/data/fr3_lerobot/left_fr3_ip_dst_q2cq",
    "episodes": null,
    "streaming": false,
    "use_imagenet_stats": true,
    "image_transforms": {
      "enable": true
    }
  },
  "policy": {
    "type": "act",
    "device": "cuda",
    "push_to_hub": false,
    "use_amp": true,
    "pretrained_path": null,
    "optimizer_lr": 1e-05,
    "optimizer_lr_backbone": 1e-05,
    "optimizer_weight_decay": 0.0001
  },
  "use_policy_training_preset": false,
  "optimizer": {
    "type": "adamw",
    "lr": 0.0003,
    "weight_decay": 0.02,
    "betas": [
      0.9,
      0.999
    ],
    "eps": 1e-08,
    "grad_clip_norm": 1.0
  },
  "scheduler": {
    "type": "cosine_decay_with_warmup",
    "num_warmup_steps": 1000,
    "num_decay_steps": 20000,
    "peak_lr": 0.0003,
    "decay_lr": 3e-05
  },
  "batch_size": 8,
  "num_workers": 4,
  "steps": 20000,
  "eval_freq": 1000,
  "log_freq": 100,
  "save_checkpoint": true,
  "save_freq": 10000,
  "seed": 42,
  "wandb": {
    "enable": true,
    "disable_artifact": true
  },
  "output_dir": "outputs/train/act_ot_B_ot_tune",
  "ot": {
    "enable": true,
    "src_repo_id": "left_fr3_ip_src_q2cq",
    "src_root": "/data/fr3_lerobot/left_fr3_ip_src_q2cq",
    "pair_info_path": "src/lerobot/datasets/hirol/dtw/source_to_target_state_joint_gripper_pair_info.json",
    "batch_ratio": 0.5,
    "lambda_ot": 0.2,
    "obs_keys": [
      "observation.images.third_person_cam_color",
      "observation.images.ee_cam_color",
      "observation.images.side_cam_color"
    ],
    "base_index_src": 0,
    "base_index_tgt": 0,
    "window_size": 10,
    "loss_config": {
      "features": [
        {
          "src_key": "observation.images.third_person_cam_color",
          "tgt_key": "observation.images.third_person_cam_color",
          "use_learned_embed": true,
          "weight_embed": 0.3333,
          "weight_label": 0.0,
          "term_name": "img_third_person"
        },
        {
          "src_key": "observation.images.ee_cam_color",
          "tgt_key": "observation.images.ee_cam_color",
          "use_learned_embed": true,
          "weight_embed": 0.3333,
          "weight_label": 0.0,
          "term_name": "img_ee"
        },
        {
          "src_key": "observation.images.side_cam_color",
          "tgt_key": "observation.images.side_cam_color",
          "use_learned_embed": true,
          "weight_embed": 0.3333,
          "weight_label": 0.0,
          "term_name": "img_side"
        },
        {
          "src_key": "action",
          "tgt_key": "action",
          "dim_slice": [
            0,
            8
          ],
          "use_learned_embed": false,
          "embed_name": null,
          "weight_embed": 0.0,
          "weight_label": 0.02,
          "term_name": "action_lbl"
        }
      ],
      "reg": 0.07,
      "tau_src": 0.5,
      "tau_tgt": 0.5,
      "heuristic": true
    }
  }
}